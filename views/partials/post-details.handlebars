<section class="card">
  <a href="/post/{{id}}">
    <header>{{title}}</header>
    <h4>{{content}}</h4>
    <h4>Date Created: {{created}}</h4>
  </a>
</section>


<div class="card" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">{{title}}</h5>
    <p class="card-text">{{content}}</p>
    <p class=""">{{created}}</p>
  </div>
</div>